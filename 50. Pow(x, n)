# 2016-02-28  Runtime: 56 ms
# Binary rearch
class Solution(object):
    def myPow(self, x, n):
        """
        :type x: float
        :type n: int
        :rtype: float
        """
        if n==0:
            return 1
        if n < 0:
            return 1 / self.myPow_help(x, -n)
        return self.myPow_help(x, n)
        
    def myPow_help(self,x,n):
        if n==2:
            return x*x
        if n==1:
            return x
        if n%2==1:
            return self.myPow_help(x*x,n/2)*x
        return self.myPow_help(x*x,n/2)
