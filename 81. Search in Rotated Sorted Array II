# 2016-10-22
public class Solution {
    public boolean search(int[] A, int target) {
        if (A == null || A.length == 0)  return false;
        int n = A.length;
        for (int i = 0; i < n; i++) {
            if (A[i] == target) {
                return true;
            }
        }
        return false;
    }
}
