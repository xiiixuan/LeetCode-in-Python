# 2016-02-22  Runtime: 40 ms  79.89%
class Solution(object):
    def findMin(self, nums):
        L,R=0,len(nums)-1
        if L==R:
            return nums[L]
        if nums[L]<nums[R]:
            return nums[L]
        while L<R:
            if R-L==1:
                return min(nums[L],nums[R])
            M=(L+R)/2
            if nums[L]<nums[M]:
                L=M
            else:
                R=M
