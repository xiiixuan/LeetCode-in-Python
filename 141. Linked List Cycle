# 2016-04-25
/**
 * Definition for singly-linked list.
 * class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) {
 *         val = x;
 *         next = null;
 *     }
 * }
 */
public class Solution {
    public boolean hasCycle(ListNode head) {  
        // write your code here
        ListNode slow =  head;
        ListNode fast =  head;
        while (fast != null && fast.next != null) {
            fast = fast.next.next;
            slow =  slow.next;
            if (fast == slow) {
                return true;
            }
        }
        return false;
    }
}
